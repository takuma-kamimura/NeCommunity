<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>

<%if logged_in?%>
  <%= link_to '新規投稿ページ',new_post_path %>
  <%= link_to 'いいね一覧',likes_posts_path %>
  <%= link_to 'ブックマーク一覧',bookmarks_posts_path %>
  <% if current_user.cats.present? %>
    <%= link_to 'あなたの猫種と同じ猫種に関する投稿',samebreedcats_posts_path %>
    <% current_user.cats.each do |cat| %>
      <%= link_to "#{cat.cat_breed.name}の投稿一覧" ,specifycats_posts_path(cat_breed_id: cat.cat_breed.id) %>
    <%end%>
  <%end%>
<%end%>

<%= render 'search', q: @q, url: posts_path %>

<%= render @posts %>